@LZ4_TRUE@subdir-m += lz4

MODULES := compression

compression-objs := cmp_pool.o
@LZ4_TRUE@compression-objs += lz4/lz4_compress.o lz4/lz4_decompress.o

EXTRA_DIST := $(compression-objs:.o=.c)
EXTRA_DIST += cmp_pool.h

@LZ4_TRUE@EXTRA_DIST += lz4/lz4defs.h lz4/lz4.h

EXTRA_PRE_CFLAGS := -O3

@INCLUDE_RULES@
